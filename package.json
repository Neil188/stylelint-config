{
    "name": "@neil188/stylelint-config",
    "version": "1.5.0",
    "description": "Stylelint rules",
    "main": "index.js",
    "scripts": {
        "stylelint-find-unused-rules": "stylelint-find-rules --config ./index.js",
        "eslint": "eslint *.js",
        "printconfig": "stylelint --config index.js test/index.css --print-config",
        "stylelint": "stylelint --config index.js test/*.css",
        "test": "npm run eslint && npm run stylelint"
    },
    "keywords": [
        "stylelint",
        "config"
    ],
    "author": "N Lupton",
    "license": "MIT",
    "peerDependencies": {},
    "dependencies": {
        "@neil188/browserslist-config": "^2.4.1",
        "stylelint": "^10.1.0",
        "stylelint-a11y": "^1.2.1",
        "stylelint-config-standard": "^19.0.0",
        "stylelint-no-unsupported-browser-features": "^3.0.2",
        "stylelint-order": "^3.1.0"
    },
    "engines": {
        "node": "10.16.x",
        "npm": "6.10.x"
    },
    "files": [
        "rules"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Neil188/stylelint-config.git"
    },
    "devDependencies": {
        "@neil188/eslint-config": "^1.9.0",
        "eslint": "6.1.0",
        "stylelint-find-rules": "^2.1.0"
    },
    "browserslist": [
        "extends @neil188/browserslist-config"
    ]
}
