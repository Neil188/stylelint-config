{
    "name": "@neil188/stylelint-config",
    "version": "1.3.0",
    "description": "Stylelint rules",
    "main": "index.js",
    "scripts": {
        "stylelint-find-unused-rules": "stylelint-find-rules --config ./index.js",
        "eslint": "eslint *.js",
        "printconfig": "stylelint --config index.js test/index.css --print-config",
        "stylelint": "stylelint --config index.js *.css",
        "test": "npm run eslint && npm run stylelint"
    },
    "keywords": [
        "stylelint",
        "config"
    ],
    "author": "N Lupton",
    "license": "MIT",
    "peerDependencies": {},
    "dependencies": {
        "@neil188/browserslist-config": "^2.3.0",
        "stylelint": "^9.10.1",
        "stylelint-a11y": "^1.1.11",
        "stylelint-config-standard": "^18.3.0",
        "stylelint-no-unsupported-browser-features": "^3.0.2",
        "stylelint-order": "^2.2.1"
    },
    "engines": {
        "node": "8.11.x",
        "npm": "6.2.x"
    },
    "files": [
        "rules"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Neil188/stylelint-config.git"
    },
    "devDependencies": {
        "@neil188/eslint-config": "^1.8.0",
        "eslint": "5.6.0",
        "stylelint-find-rules": "^2.0.0"
    },
    "browserslist": [
        "extends @neil188/browserslist-config"
    ]
}
